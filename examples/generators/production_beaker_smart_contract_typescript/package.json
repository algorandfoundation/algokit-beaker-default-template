  },
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^3.0.3",
    "@types/jest": "^29.5.11",
    "dotenv": "^16.0.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
